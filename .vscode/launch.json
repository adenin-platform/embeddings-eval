{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Generate Embeddings - Default Dataset",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/index.js",
      "args": ["generate", "--dataset", "default", "--model", "default"],
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Validate Project - Default",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/validate-project.js",
      "args": ["default"],
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Generate Embeddings - Courses DE",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/index.js",
      "args": ["generate", "--dataset", "courses-de", "--model", "default"],
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Generate Embeddings - Intranet",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/index.js",
      "args": ["generate", "--dataset", "intranet", "--model", "default"],
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Validate Project - Courses DE",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/validate-project.js",
      "args": ["courses-de"],
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Evaluate - Default Dataset",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/index.js",
      "args": ["evaluate", "--dataset", "default", "--model", "default"],
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Full Pipeline - Default Dataset",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/index.js",
      "args": ["--dataset", "default", "--model", "default"],
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Full Pipeline - Intranet Dataset",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/index.js",
      "args": ["--dataset", "intranet", "--model", "default"],
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}